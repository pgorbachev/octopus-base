<?xml version="1.0" encoding="UTF-8"?>
<template xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>Octopus Release</name>
  <settings>
    <options>
      <option name="buildNumberPattern" value="%BUILD_NUMBER%" />
      <option name="maximumNumberOfBuilds" value="1" />
    </options>
    <parameters>
      <param name="OCTOPUS_RELEASE_TIMEOUT" value="10" />
    </parameters>
    <build-runners>
      <runner id="RUNNER_4500" name="Call GitHub action" type="OctopusCallGitHubAction">
        <parameters>
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings />
    <requirements />
    <build-triggers />
    <cleanup />
  </settings>
</template>

